#include <stdio.h>

int findDuplicate(int arr[], int n) {
    int freq[n+1];  // frequency array
    for(int i = 0; i <= n; i++)
        freq[i] = 0;

    for(int i = 0; i <= n; i++) {
        if(freq[arr[i]] == 1)
            return arr[i]; // duplicate found
        freq[arr[i]]++;
    }

    return -1; // no duplicate (should not happen)
}

int main() {
    int arr[] = {1, 3, 4, 2, 2};
    int n = sizeof(arr)/sizeof(arr[0]) - 1; // n = max number

    int duplicate = findDuplicate(arr, n);
    printf("Duplicate element = %d\n", duplicate);

    return 0;
}
